<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_users"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.ahmedsayed.privatechat.UsersActivity">
    <include layout="@layout/app_bar_layout" android:id="@+id/user_appbar"/>
<android.support.v7.widget.RecyclerView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_below="@+id/user_appbar"
    android:id="@+id/users_list"></android.support.v7.widget.RecyclerView>
</RelativeLayout>
